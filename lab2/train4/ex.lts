const N = 5
EAST = (arrive -> EAST).
WEST = (leave -> WEST).
DIRECTOR = (open -> close -> DIRECTOR).
CONTROL = CLOSED[0],
CLOSED[i:0..N] = (when (i==0) open -> OPENED[0]
				|when (i>0) leave -> CLOSED[i-1]
				),
OPENED[i:0..N] = (close -> CLOSED[i]
					|when (i<N) arrive -> OPENED[i+1]
					|when (i>0) leave -> OPENED[i-1]
					).
||MUSEUM = (EAST || WEST || DIRECTOR || CONTROL).
